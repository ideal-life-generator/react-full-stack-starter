{
  "name": "modern-react-starter-kit",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "dev:client": "better-npm-run dev:client",
    "dev:server": "better-npm-run dev:server",
    "dev:api": "better-npm-run dev:api",
    "dev": "concurrently \"npm run dev:client\" \"npm run dev:server\" \"npm run dev:api\""
  },
  "betterScripts": {
    "dev:client": {
      "command": "webpack-dev-server --config webpack-client.js",
      "env": {
        "NODE_ENV": "development",
        "RENDERING_ON": "browser"
      }
    },
    "dev:server": {
      "command": "webpack --watch --config webpack-server.js --display-modules --display-chunks",
      "env": {
        "NODE_ENV": "development",
        "RENDERING_ON": "server"
      }
    },
    "dev:api": {
      "command": "webpack --watch --config webpack-api.js --display-modules --display-chunks",
      "env": {
        "NODE_ENV": "development"
      }
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ideal-life/modern-react-starter-kit.git"
  },
  "author": "ideal.life",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ideal-life/modern-react-starter-kit/issues"
  },
  "homepage": "https://github.com/ideal-life/modern-react-starter-kit#readme",
  "devDependencies": {

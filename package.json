{
  "name": "modern-react-starter-kit",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "dev:client": "better-npm-run dev:client",
    "dev:server": "better-npm-run dev:server",
    "dev": "parallelshell \"better-npm-run dev:client\" \"better-npm-run dev:server\"",
    "prod": "better-npm-run prod"
  },
  "betterScripts": {
    "dev:client": {
      "command": "webpack-dev-server --config webpack-client.js",
      "env": {
        "NODE_ENV": "development"
      }
    },
    "dev:server": {
      "command": "webpack --watch --config webpack-server.js",
      "env": {
        "NODE_ENV": "development"
      }
    },
    "prod": {
      "command": "webpack",
      "env": {
        "NODE_ENV": "production",
        "TARGET": "browser"
      }
    },
    "prod:server": {
      "command": "webpack --config webpack.config.serv.js",
      "env": {
        "NODE_ENV": "production",
        "TARGET": "server"
      }
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ideal-life/modern-react-starter-kit.git"
  },
  "author": "ideal.life",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ideal-life/modern-react-starter-kit/issues"
  },
  "homepage": "https://github.com/ideal-life/modern-react-starter-kit#readme",
